{
    "name": "tipoff/staff-bookings",
    "description": "Laravel Package for Staff Bookings in Nova.",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "tipoff/authorization": "^2.7.0",
        "tipoff/bookings": "^2.2.1",
        "tipoff/checkout": "^2.5.2",
        "tipoff/support": "^1.8.7"
    },
    "require-dev": {
        "tipoff/test-support": "^1.4.0"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\StaffBookings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\StaffBookings\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\StaffBookings\\ToolServiceProvider"
            ]
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
